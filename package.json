{
  "name": "@nrfcloud/cdktf-aws-adaptor",
  "version": "0.0.0",
  "description": "A compatibility layer for using the CDK for Terraform with AWS CDK constructs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "typescript",
    "cdktf",
    "awscdk",
    "cdk",
    "aws"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "!dist/tsconfig.tsbuildinfo",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {},
  "repository": "https://github.com/nRFCloud/ts-json-schema-transformer",
  "scripts": {
    "prepare": "husky install",
    "lint": "dprint check && denolint --config .denolint.json",
    "lint:fix": "dprint fmt && denolint --config .denolint.json",
    "clean": "rm -rf dist && rm -rf tests/dist && rm -rf src/generated",
    "compile": "tsc",
    "test": "vitest",
    "prepublish": "pnpm clean && pnpm compile && pnpm test",
    "release": "changeset publish",
    "update-cc-supported": "node scripts/update-cc-supported.js"
  },
  "author": "John Conley",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.421.0",
    "@cdktf/provider-aws": ">=18.0.1",
    "@changesets/cli": "^2.26.2",
    "@eslint/js": "^8.52.0",
    "@types/debug": "^4.1.8",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "aws-cdk-lib": ">=2.101.0",
    "cdktf": "^0.19.0",
    "cdktf-cli": "^0.18.2",
    "constructs": "^10.2.69",
    "dprint": "^0.35.3",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "type-fest": "^4.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@cdktf/provider-aws": ">=18.0.1",
    "aws-cdk-lib": ">=2.101.0",
    "cdktf": "^0.19.0",
    "constructs": "^10.2.69"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "codemaker": "^1.87.0",
    "debug": "^4.3.4",
    "yargs": "^17.7.2"
  }
}
